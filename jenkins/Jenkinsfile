pipeline {
    agent any

    stages {
        stage('Dev') {
            steps {
                echo 'git clone'
                echo 'Start Ansible playbook..'
                echo 'Complete.'
                echo '------------------------'
            }

        }
        stage('Test') {
            steps {
                echo 'Start Integration Test..'
                echo 'Test Complete'
                echo '------------------------'
                echo 'Start Unit Test..'
                echo 'Test Complete'
                echo '------------------------'
            }
        }
        stage('Prod') {
            steps {
                echo 'Deploy to prod..'
                echo 'Complete'
                echo '------------------------'
            }
        }

    }
}
