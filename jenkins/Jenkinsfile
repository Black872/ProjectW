pipeline {
    agent main
    options {
        buildDiscarder(logRotator(numToKeepStr: '5', artifactNumToKeepStr: '10'))
    }

    stages {
        stage('Build') {
            
            steps {
                echo 'git status'
                echo 'Start Ansible playbook..'
                sh 'git clone -b test git@github.com:Black872/ProjectW.git'
                sh 'ls -la'
                echo 'Complete.'
                echo '------------------------'
                echo 'Version 3'
                
            
            }

        }
        

    }
}
