pipeline {
    agent any
    environment {
        CC = """${sh(
                returnStdout: true,
                script: 'echo "clang"'
        )}"""
        EXIT_STATUS = """${sh(
            returnStatus: true,
            script: 'exit 1'
        )}"""
    }
    options {
        timeout(time: 5, unit: 'SECONDS')
    }

    stages {
        stage('Build') {
            environment {
                DEBUG_FLAGS = '-g'
            }
            steps {
                echo 'git status'
                echo 'Start Ansible playbook..'
                echo 'Complete.'
                echo '------------------------'
                sh 'printenv'
                
            
            }

        }
        

    }
}
