pipeline {
    agent any

    options {
    // Timeout counter starts AFTER agent is allocated
        timeout(time: 3, unit: 'SECONDS')
    }

    stages {
        stage('Build') {
            
            steps {
                echo 'git status'
                echo 'Start Ansible playbook..'
                sh 'git clone -b test git@github.com:Black872/ProjectW.git'
                sh 'ls -la'
                echo 'Complete.'
                echo '------------------------'
                echo 'Version 1'
                
            
            }

        }
        

    }
}
