pipeline {
    agent any
    

    stages {
        stage('Build') {
            steps {
                echo 'git status'
                echo 'Start Ansible playbook..'
                echo 'Complete.'
                echo '------------------------'
                
            
            }

        }
        stage('Test') {
            steps {
                echo 'Start Integration Test..'
                echo 'Test Complete'
                echo '------------------------'
                echo 'Start Unit Test..'
                echo 'Test Complete'
                echo '------------------------'
                
            }

        }
        stage('Prod') {
            steps {
                echo 'Deploy to prod..'
                echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
                echo 'Complete'
                echo '------------------------'

            }
        }

    }
}
